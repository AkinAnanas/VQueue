{
  "name": "Python Dev",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "service": "backend",
  "workspaceFolder": "/app",
  "forwardPorts": [
    8000
  ],
  "portsAttributes": {
    "8000": {
      "onAutoForward": "openPreview"
    }
  },
  "extensions": [
    "ms-python.python",
    "ms-python.vscode-pylance",
    "ms-azuretools.vscode-docker"
  ],
  "settings": {
    "python.defaultInterpreterPath": "/usr/local/bin/python"
  },
}